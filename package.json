{
  "name": "vue-agreed",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --port 20000",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "agreed:server": "agreed-server --path ./agreed/mock.js --port 21000",
    "agreed:ui": "agreed-ui --path ./agreed/mock.js --port 21001",
    "agreed:ui:build": "agreed-ui build --path ./agreed/mock.js --dest ./build",
    "agreed": "run-p agreed:server agreed:ui"
  },
  "dependencies": {
    "agreed": "3.2.2",
    "agrios": "1.0.0",
    "axios": "0.19.0",
    "core-js": "3.6.5",
    "vue": "2.6.12",
    "vue-router": "3.4.3",
    "vuex": "3.5.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "4.5.4",
    "@vue/cli-plugin-eslint": "4.5.4",
    "@vue/cli-plugin-router": "4.5.4",
    "@vue/cli-plugin-vuex": "4.5.4",
    "@vue/cli-service": "4.5.4",
    "@vue/eslint-config-prettier": "5.0.0",
    "agreed-core": "3.2.5",
    "babel-eslint": "10.1.0",
    "body-parser": "1.19.0",
    "eslint": "7.7.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-vue": "6.2.2",
    "express": "4.17.1",
    "lint-staged": "10.2.13",
    "npm-run-all": "4.1.5",
    "prettier": "2.1.1",
    "vue-template-compiler": "2.6.12"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/prettier"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}